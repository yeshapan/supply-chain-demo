{
  "catalogs": [
    {
      "name": "supply_chain_sql",
      "type": "postgres",
      "metastore": {
        "host": "supply_chain_db",
        "port": 5432,
        "user": "admin",
        "password": "password",
        "database": "supplychain",
        "ssl": false
      }
    }
  ],
  "graph": {
